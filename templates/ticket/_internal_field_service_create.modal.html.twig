<div class="modal fade" id="modalnewinternalincident" tabindex="-1" aria-labelledby="modalnewinternalincidentLabel" aria-hidden="true" data-backdrop="static">
    <div class="modal-dialog modal-dialog-scrollable modal-xl">
    <div class="modal-content">
      <div class="modal-header">  
        <h5 class="modal-title text-center" id="modalnewinternalincidentLabel">{{'fieldservice.index.button'|trans}}</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="container-fluid">
          {{form_start(internalFSForm)}}
            <div class="form-group row">
              <div class="col-12 col-md-2">
                {{ form_label(internalFSForm.problem)}}
              </div>
              <div class="col-12 col-md-10">
                {{form_widget(internalFSForm.problem)}}
              </div>
            </div>
            <div class="form-group row">
              <div class="col-12">
                {{form_widget(internalFSForm.description)}}
              </div>
            </div>
            <div class="form-group row ">
              <div class="col-12 col-md-6 text-left">
                {{form_widget(internalFSForm.save)}}
              </div>
              <div class="col-12 col-md-6 text-right">
                {{form_widget(internalFSForm.reset)}}
              </div>
            </div>
          {{form_end(internalFSForm)}}
        </div>
      </div>
    </div>
    </form>
  </div>
</div>