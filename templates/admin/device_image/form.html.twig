<div class="card">
	<h5 class="card-header text-center">
		{{'admin.index.images'|trans}}
	</h5>
	<div class="card-body">
		{{ form_start(form)}}
		<div class="form-group row">
			<div class="col-sm-2 align-self-center">
				{{ form_label(form.devicemodel) }}
			</div>
			<div class="col-sm-10">
				{{ form_widget(form.devicemodel, { 'attr': { 'class' : 'selectpicker', 'data-live-search' : true, 'title': 'Toestel model' } }) }}
			</div>
		</div>
		<div class="form-group row">
			<div class="col-sm-2 align-self-center">
				{{ form_label(form.image)}}
			</div>
			<div class="col-sm-10">
				{{ form_widget(form.image, { 'attr': {'class': 'form-control' } }) }}
			</div>
		</div>
		<div>
			<button id="saveImage" type="submit" class="btn btn-primary">Bewaren</button>
        </div>
		{{ form_end(form)}}
	</div>
</div>