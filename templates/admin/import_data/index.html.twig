{% extends 'base.html.twig' %}

{% block body %}
<div class="container-fluid mb-3 mt-3">
  <div class="row">
    <div class="col-md-12">
      <div class="card bg-light mb-3 mt-3">
        <div class="card-body">
          <h5 class="card-header">Lijst met accounts</h5>
          <table class="table" style="width: 100%">
            <tr>
              <th>Email</th>
              <th>Naam</th>
            </tr>
            {% for item in data %}
            <tr>
              <td>{{ item.name }}</td>
              <td>{{ item.email }}</td>
            </tr>
            {% endfor %}
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
{% endblock %}
